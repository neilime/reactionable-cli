{
  "name": "@reactionable/cli",
  "version": "0.0.1",
  "description": "Reactionable cli",
  "main": "dist/index",
  "bin": "./bin/reactionable",
  "scripts": {
    "start": "npm link && tsc-watch --onSuccess \"npm run build:copy-templates\"",
    "build": "rimraf ./dist && tsc && npm run build:copy-templates",
    "build:copy-templates": "cd src && copyfiles ./**/*.template ../dist/ && cd ..",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactionable/reactionable-cli.git"
  },
  "keywords": [
    "reactionable",
    "cli",
    "tools"
  ],
  "author": "ESCEMI <developer@escemi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactionable/reactionable-cli/issues"
  },
  "homepage": "https://github.com/reactionable/reactionable-cli#readme",
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/inversify": "^2.0.33",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.7.12",
    "copyfiles": "^2.1.1",
    "rimraf": "^3.0.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "esm": "^3.2.25",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.0",
    "inversify": "^5.0.1",
    "js-ini": "^1.1.2",
    "mustache": "^3.1.0",
    "parse-github-url": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.3"
  }
}
