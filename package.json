{
  "name": "@reactionable/cli",
  "version": "0.0.1",
  "description": "Reactionable cli",
  "main": "dist/index.js",
  "bin": "./bin/reactionable",
  "scripts": {
    "start": "npm link && tsc-watch --onSuccess \"npm run build:copy-templates\"",
    "build": "rimraf ./dist && tsc && npm run build:copy-templates && npm run build:doc",
    "build:copy-templates": "rimraf ./dist/**/*.template && cd src && copyfiles ./**/*.template ../dist/ && cd ..",
    "build:doc": "rimraf -rf docs && typedoc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactionable/reactionable-cli.git"
  },
  "keywords": [
    "reactionable",
    "cli",
    "tools"
  ],
  "author": "ESCEMI <contact@escemi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactionable/reactionable-cli/issues"
  },
  "homepage": "https://reactionable.github.io/reactionable-cli",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/chalk": "^2.2.0",
    "@types/diff": "^4.0.2",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^25.2.1",
    "@types/mustache": "^4.0.0",
    "@types/node": "^13.1.0",
    "@types/parse-github-url": "^1.0.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/shelljs": "^0.8.6",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.3.1",
    "tsc-watch": "^4.0.0",
    "typedoc": "^0.17.0",
    "typedoc-plugin-nojekyll": "^1.0.1"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "@typescript-eslint/typescript-estree": "^2.8.0",
    "chalk": "^4.0.0",
    "deepmerge": "^4.2.2",
    "diff": "^4.0.1",
    "esm": "^3.2.25",
    "figlet": "^1.2.4",
    "handlebars": "^4.7.6",
    "inquirer": "^7.0.0",
    "inversify": "^5.0.1",
    "js-ini": "^1.2.0",
    "parse-github-url": "^1.0.2",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "prepare-commit-msg",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
