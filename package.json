{
  "name": "@reactionable/cli",
  "version": "1.2.8",
  "description": "Reactionable cli",
  "main": "dist/index.js",
  "bin": {
    "reactionable": "./bin/reactionable"
  },
  "scripts": {
    "start": "yarn link && tsc-watch",
    "build": "rimraf ./dist && yarn lint && tsc && yarn build:copy-templates",
    "build:copy-templates": "rimraf ./dist/**/*.template && cd src && copyfiles ./**/*.template ../dist/ && cd ..",
    "build:doc": "rimraf -rf docs && typedoc",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactionable/reactionable-cli.git"
  },
  "keywords": [
    "reactionable",
    "cli",
    "tools"
  ],
  "author": "ESCEMI <contact@escemi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactionable/reactionable-cli/issues"
  },
  "homepage": "https://reactionable.github.io/reactionable-cli",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/chalk": "^2.2.0",
    "@types/diff": "^4.0.2",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^7.3.0",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^26.0.9",
    "@types/mock-fs": "^4.10.0",
    "@types/mustache": "^4.0.0",
    "@types/node": "^14.0.27",
    "@types/parse-github-url": "^1.0.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.8.0",
    "commitizen": "^4.1.2",
    "copyfiles": "^2.3.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.3",
    "jest": "^26.2.2",
    "mock-fs": "^4.12.0",
    "mock-spawn": "^0.2.6",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.1.4",
    "tsc-watch": "^4.2.9",
    "typedoc": "^0.17.8",
    "typedoc-plugin-nojekyll": "^1.0.1"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@typescript-eslint/typescript-estree": "^3.6.1",
    "chalk": "^4.1.0",
    "deepmerge": "^4.2.2",
    "diff": "^4.0.1",
    "esm": "^3.2.25",
    "figlet": "^1.5.0",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.3",
    "inversify": "^5.0.1",
    "js-ini": "^1.2.0",
    "parse-github-url": "^1.0.2",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.4",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
